					['EUR', '&euro;'],
					['GBP', '&#8356;'],
					['CAD', '&#36;'],
					['AUD', '&#36;']
				];

				if (!currencySelect) {
					$('.currency-select').html('USD &#36;').data('currency', 'USD');
				} else {
					$(currencySigns).each(function (idx, el) {
						if (currencySelect == $(el)[0]) {
							currencySelect = currencySelect + ' ' + $(el)[1];
						}
					});
					$('.currency-select').html(currencySelect);
				}

				$('.all-currencys').children().each(function (idx, el) {
					$(this).on('click', function (e) {
						var currentCurrency = $('.header-opt .currency-select').html(),
							allCurrencyOpts = $('.cr-select.custom-dp-select .select-items > div');
						var currency = $(this).data('currency');

						$(currencySigns).each(function (indx, ele) {
							if (currency === $(ele)[0]) {
								sign = $(ele)[1];
							}
						});

						var arrCurrencies = {
							'USD': 'USD $',
							'EUR': 'EUR €',
							'GBP': 'GBP ₤',
							'CAD': 'CAD $',
							'AUD': 'AUD $'
						};

						for (var c = 0; c < allCurrencyOpts.length; c++) {
							if (arrCurrencies[currency] == $(allCurrencyOpts[c]).text()) {
								$(allCurrencyOpts[c]).text(currentCurrency);
							}
						}

						localStorage.setItem('default_currency', currency);

						$('.currency-select').html(currency + ' ' + sign);
						$('#oCurrency').prop('value', currency);
						$('#calcCurrencySwitcher > option').each(function (idx, el) {
							if ($(el).val() == currency) {
								$(el).prop('selected', 'true');
							}
						});

						$('#calcCurrencySwitcher').trigger('change');

						$('#calcStickyCurrencySwitcher > option').each(function (idx, el) {
							if ($(el).val() == currency) {
								$(el).prop('selected', 'true');
							}
						});

						$('#calcStickyCurrencySwitcher').trigger('change');
					});
				});

			});
