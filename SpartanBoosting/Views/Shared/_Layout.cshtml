			$.ajax({
				url: '/Account/LoginAjax',
				data: {
					Email: $('.login-username').val(),
					Password: $('.login-password').val()
				},
				type: 'POST',
				success: function (response) {
					if (!response.success) {
						$('#LoginModel_loginattempt-error').text('Invalid Login Attempt').show()
					}
					else {
						window.location.href = response.redirectToUrl;
					}
				}
			});
		});
		$(".forgot-password").click(function (event) {
			debugger;
			event.preventDefault();
			$('#forgottenattempt-error').hide()
			$.ajax({
				url: '/Account/ForgotPasswordAjax',
				data: {
					Email: $('.forgot-password-email').val(),
				},
				type: 'POST',
				success: function (response) {
					if (!response.success) {
						$('#forgottenattempt-error').addClass('nk-error').removeClass('nk-success')
						$('#forgottenattempt-error').text(response.message).show()
					}
					else {
						$('#forgottenattempt-error').removeClass('nk-error').addClass('nk-success')
