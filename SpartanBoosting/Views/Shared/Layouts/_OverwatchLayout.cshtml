<script type="text/javascript">
	$('.dropdown-toggle').on('click', function () {
		debugger;
		if (!$(this).parent().hasClass('showing-dropdown')) {
			$('.nav-menu-dropdown:visible').hide()
			$("body").find("[aria-labelledby='" + $(this).attr('id') + "']").first().stop(true, true).slideDown();
			$(this).parent().addClass('showing-dropdown')
		}
		else {
			$("body").find("[aria-labelledby='" + $(this).attr('id') + "']").first().stop(true, true).slideUp();
			$(this).parent().removeClass('showing-dropdown')
		}
	});

</script>