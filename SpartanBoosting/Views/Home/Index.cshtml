@{
	Layout = "~/Views/Shared/_MainPageLayout.cshtml";
}
<div class="nk-header-title nk-header-title-md nk-header-title-parallax nk-header-title-parallax-opacity" style="height: 84.5vh;">
	<div class="bg-image op-6">
		<img src="~/Client/assets/images/LeagueOfLegends/main-backgrounmd.jpg" />
	</div>
	<div class="d-flex align-items-center w-100">
		<div class="row w-100">
			<div class="col-lg-12">
				<div class="row no-gutters">
					<div class="col-md-4"></div>
					<a class="col-md-4 pr-5" href="../LolBoosting/SoloBoosting" style="		color: #ffffff;cursor: pointer;text-decoration: none;">
						<div class="nk-box-3 bg-dark-1 league-link">
						</div>
					</a>
					<div class="col-md-4"></div>
				</div>
			</div>
		</div>
		<div class="nk-gap-4"></div>
		<div class="nk-gap-4"></div>
	</div>


</div>

<div class="nk-box bg-dark-1">
	<div class="container text-center">
		<div class="nk-gap-6"></div>
		<div class="nk-gap-2"></div>
		<h2 class="nk-title h1">Advantages of the service</h2>
		<div class="nk-gap-3"></div>
		<p class="lead">
			We provide Boosting services in games LoL, TFT and WoW. Spartan Boosting is a global boosting solution.
			Apart from Boosting, Spartan Boosting offers Coaching and Account sales services.
		</p>
		<div class="nk-gap-2"></div>
		<div class="row no-gutters">
			<div class="col-md-4">
				<div class="nk-box-2 nk-box-line">
					<!-- START: Counter -->
					<div class="nk-counter-3">
						<div class="nk-count">100+</div>
						<h3 class="nk-counter-title h4">Professional Boosters</h3>
						<label class="nk-counter-title">Our team includes the top challenger LoL boosters. We hand-select & screen our employees, who provide professional performance & attitude.</label>
						<div class="nk-gap-1"></div>
					</div>
					<!-- END: Counter -->
				</div>
			</div>
			<div class="col-md-4">
				<div class="nk-box-2 nk-box-line">
					<!-- START: Counter -->
					<div class="nk-counter-3 row d-flex justify-content-center">
						<div class="row col-md-12 d-flex justify-content-center">
							<div class="nk-count">24</div> <span class="pr-2 pl-2 nk-count-style">/</span> <div class="nk-count">7</div>
						</div><div class="row col-md-12 d-flex justify-content-center">
							<h3 class="nk-counter-title h4">Availability</h3>
						</div>

						<div class="nk-gap-1"></div>
					</div>
					<!-- END: Counter -->
				</div>
			</div>
			<div class="col-md-4">
				<div class="nk-box-2 nk-box-line nk-box-last">
					<!-- START: Counter -->
					<div class="nk-counter-3">
						<div class="nk-count">
							50
						</div>
						<div>
							<h3 class="nk-counter-title h4">Boosters Ready to Start</h3>
							<div class="nk-gap-1"></div>
						</div>
						<!-- END: Counter -->
					</div>
				</div>
			</div>
			<div class="nk-gap-6"></div>
			<div class="nk-gap-6"></div>
		</div>
	</div>
</div>
